[global_config]
[keybindings]
  zoom_in = <Primary><Shift>plus
  zoom_out = <Primary><Shift>underscore
[profiles]
  [[default]]
    allow_bold = False
    background_color = "#f3f4d9"
    cursor_color = "#aaaaaa"
    font = MesloLGS NF 12
    foreground_color = "#2e3436"
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
